        "# 台形の各辺の長さ（cm）\n",
        "top = 10      # 上辺\n",
        "bottom = 20   # 下辺\n",
        "height = 5    # 高さ\n",
        "\n",
        "# 台形の面積を計算\n",
        "area = (top + bottom) * height / 2\n",
        "\n",
        "# 結果を表示\n",
        "print(f\" {area} cm²\")\n"
      ],
